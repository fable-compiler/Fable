{
  "compilerOptions": {
    "target": "es2020",
    "module": "es2020",
    "sourceMap": false,
    "allowJs": true,
    "esModuleInterop": true,
    "strict": true,
    // "skipLibCheck": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "noEmitOnError": true,
    "newLine": "lf",
    "forceConsistentCasingInFileNames": true,
    "preserveWatchOutput": true
  }
}
