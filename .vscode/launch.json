    {
      "name": "Fable.Cli - fable-library-dart",
      "type": "coreclr",
      "request": "launch",
      "program": "dotnet",
      "args": [
        "run",
        "--project",
        "${workspaceRoot}/src/Fable.Cli",
        "--",
        "src/fable-library-dart",
        "--outDir",
        "temp/fable-library-dart",
        "--fableLib",
        "./temp/fable-library-dart",
        "--lang",
        "dart",
        "--exclude",
        "Fable.Core",
        "--define",
        "FABLE_LIBRARY",
        "--noCache"
      ]
    },
    {
      "name": "Fable.Cli - fable-library-ts",
      "type": "coreclr",
      "request": "launch",
      "program": "dotnet",
      "args": [
        "run",
        "--project",
        "${workspaceRoot}/src/Fable.Cli",
        "--",
        "src/fable-library",
        "--outDir",
        "temp/fable-library-ts",
        "--fableLib",
        "./temp/fable-library-ts",
        "--lang",
        "typescript",
        "--exclude",
        "Fable.Core",
        "--define",
        "FABLE_LIBRARY",
        "--noCache",
        "--typedArrays",
        "false",
        "--define",
        "FX_NO_BIGINT"
      ]
    },
    {
      "name": "Fable.Cli - fable-library-py",
